[MASTER]
ignore-patterns=test_[a-zA-Z0-9_]\.py

[FORMAT]
max-line-length=120
max-args=10
variable-rgx: [a-z_][a-z0-9]{0,30}
argument-rgx: [a-z_][a-z0-9]{0,30}
attr-rgx: [a-z_][a-z0-9]{0,30}
module-rgx: (([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$

[MESSAGE CONTROL]
disable=
    duplicate-code,
    too-few-public-methods
